{
  "name": "sfdx-tools",
  "description": "A plugin to compare changes in a sfdx repo to deploy only what has changed for a deployment",
  "version": "0.0.0",
  "author": "Gavin Palmer @gavinhughpalmer",
  "bugs": "https://github.com/gavinhughpalmer/sfdx-tools/issues",
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/errors": "1.3.4",
    "@salesforce/command": "3.1.0",
    "@salesforce/core": "2.20.5",
    "decompress": "4.2.1",
    "fs": "0.0.2",
    "glob": "7.1.7",
    "glob-promise": "4.1.0",
    "sfdx-js": "4.5.0",
    "tslib": "2.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0",
    "@oclif/plugin-help": "3.2.2",
    "@oclif/test": "1.2.8",
    "@salesforce/dev-config": "1.6.0",
    "@types/chai": "4.2.17",
    "@types/mocha": "8.2.2",
    "@types/node": "10.17.59",
    "chai": "4.3.4",
    "globby": "11.0.3",
    "mocha": "8.3.2",
    "nyc": "15.1.0",
    "sinon": "9.2.4",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.2.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/gavinhughpalmer/sfdx-tools",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "gavinhughpalmer/sfdx-tools",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tslint -p test -t stylish",
    "prepare": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "fix": "tslint -p test -t stylish --fix"
  }
}
